<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ic.db.ReviewMapper">
	
	<!-- 자신의 좋아요 갯수를 조회하는 구문 -->
	<select id="getReview" resultType="com.ic.model.ReviewDTO">
		select * from review where member_id=#{member_id} AND REVIEW_CTGR = 1 AND NICKNAME IS NOT NULL
	</select>
	

</mapper>